<section class="blog_sec">
  <div class="inn_blog">
    <div class="container-fluid">
      <div class="title_blog">
        <h3>Meals Near You</h3>
      </div>
      <% @foods.each do |food| %>
        <div class="col-md-3">
          <div class="panel panel-default">
            <div class="panel-heading preview">
              <%= image_tag food.cover_photo(:medium) %>
            </div>
            <div class="panel-body">
              <div class="col-md-10">
                <h6><%= link_to food.cuisine_type, food %><br/></h6>
                <h3><%= link_to food.listing_name, food %></h3>
                $<%= food.price %> - <%= food.cuisine_type %> - <%= pluralize(food.portions_available, "Portion") %>
                <div id="star_<%= food.id %>"></div> <%= pluralize(food.foodie_reviews.count, "review") %>
              </div>
              <% if user_signed_in? %>
                <% unless (current_user && (food.user_id == current_user.id)) %>
                  <div class="col-md-2">
                    <div align="right" class="text-babu icon-babu">
                      <%= link_to "<i class='fa fa-comments fa-2x'></i>".html_safe, new_message_path(food.user_id) %>
                    </div>
                  </div>
                <% end %>
              <% end %>
            </div>
          </div>
        </div>

        <script>
           $('#star_<%= food.id %>').raty({
             path: '/assets',
             readOnly: true,
             score: <%= food.average_rating %>
           });
         </script>
      <% end %>
    </div>
  </div>
</section>

